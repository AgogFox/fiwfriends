@using Event.Models;
@model List<EventOBJ>;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers;
@{
    ViewData["Title"] = "Index";
}

@foreach(EventOBJ t in Model)
{
    <a asp-controller="Event"
       asp-action="ShowEvent"
       asp-route-title="@t.title"
       ><button style="background-color: yellowgreen">@t.title</button></a>
}

